{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["@typescript-eslint", "unused-imports", "import"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "import/no-unresolved": "off",
        "quotes": [2, "single"],
        "import/prefer-default-export": "off",
        "no-unused-vars": "off",
        "indent": [2, 4]
    }
}
